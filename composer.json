{
    "name": "burus86/api-ddd-template",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/burus86/api-ddd-template/"
        }
    ],
    "type": "project",
    "description": "API template with DDD and Hexagonal architecture",
    "keywords": [],
    "homepage": "https://github.com/burus86/api-ddd-template/",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/doctrine-bundle": "^2.3",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.8",
        "gedmo/doctrine-extensions": "^3.0",
        "nelmio/api-doc-bundle": "^4.2",
        "sensio/framework-extra-bundle": "^6.1",
        "stof/doctrine-extensions-bundle": "^1.6",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/property-info": "4.4.*",
        "symfony/proxy-manager-bridge": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/yaml": "4.4.*"
    },
    "require-dev": {
        "behat/behat": "^3.8",
        "bmitch/churn-php": "^1.5",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "friendsoftwig/twigcs": "^5.0",
        "liip/test-fixtures-bundle": "^1.11",
        "phpmd/phpmd": "^2.10",
        "phpstan/phpstan": "^0.12.93",
        "phpunit/phpunit": "^9.5",
        "povils/phpmnd": "^2.4",
        "qossmic/deptrac-shim": "^0.14.1",
        "sebastian/phpcpd": "^6.0",
        "squizlabs/php_codesniffer": "^3.6",
        "symfony/browser-kit": "^4.4",
        "symfony/css-selector": "^4.4",
        "symfony/maker-bundle": "^1.31",
        "symfony/phpunit-bridge": "^5.2",
        "wapmorgan/php-deprecation-detector": "^2.0"
    },
    "config": {
        "bin-dir": "bin/",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
